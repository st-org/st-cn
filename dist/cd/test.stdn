        {slide 2-, '\pi_1(X)'}
        \\
        &
        \pi_1(U_2)
        {to 1 -1, ar []}
        {to 2 -1, bend -20, slide 2-, ar [{slide 2-, 'j_2'}]}
    ]}]}]}
]}
